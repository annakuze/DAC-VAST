<h1>キューポイント一覧</h1>

<%= link_to '登録', new_cuepoint_path %>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>id</th>
        <th>名前</th>
        <th>作成日</th>
        <th>更新日</th>
      </tr>
    </thead>
    <tbody>
      
<% @cuepoints.each do |cuepont| %>

        <tr>
          <td><%= link_to cuepoint.id, cuepoint %></td>
          <td><%= cuepoint.name %></td>
          <td><%= cuepoint.created_at %></td>
          <td><%= cuepoint.updated_at %></td>
        </tr>
      
<% end %>

   </tbody>
  </table>
