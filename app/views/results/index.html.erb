<h1>Results</h1>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>id</th>
        <th>キャンペーン</th>
        <th>id</th>
        <th>キューポイント</th>
        <th>開始数</th>
        <th>完了数</th>
      </tr>
    </thead>
    <tbody>
      
<% @results.each do |result| %>

        <tr>
          <td><%= result.campaign.id %></td>
          <td><%= result.campaign.name %></td>
          <td><%= result.cuepoint.id %></td>
          <td><%= result.cuepoint.name %></td>
          <td><%= result.count_start %></td>
          <td><%= result.count_complete %></td>
        </tr>
      
<% end %>

   </tbody>
  </table>